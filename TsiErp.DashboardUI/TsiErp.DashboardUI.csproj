<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>disable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RunAnalyzersDuringBuild>False</RunAnalyzersDuringBuild>
    <RunAnalyzersDuringLiveAnalysis>False</RunAnalyzersDuringLiveAnalysis>
    <EnableNETAnalyzers>False</EnableNETAnalyzers>
  </PropertyGroup>
  
  <ItemGroup>
      <PackageReference Include="DevExpress.Blazor" Version="23.2.3" />
      <PackageReference Include="Microsoft.EntityFrameworkCore" Version="6.0.5" />
      <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="6.0.5" />
      <PackageReference Include="Syncfusion.Blazor.Diagram" Version="20.3.0.59" />
	  <PackageReference Include="Syncfusion.Blazor.Grid" Version="20.3.0.59" />
	  <PackageReference Include="Syncfusion.Blazor.TreeGrid" Version="20.3.0.59" />
	  <PackageReference Include="Syncfusion.Blazor.Charts" Version="20.3.0.59" />
	  <PackageReference Include="Syncfusion.Blazor.CircularGauge" Version="20.3.0.59" />
	  <PackageReference Include="Syncfusion.Blazor.Diagrams" Version="20.2.0.46" />
	  <PackageReference Include="Syncfusion.Blazor.LinearGauge" Version="20.3.0.59" />
	  <PackageReference Include="Syncfusion.Blazor.SmithChart" Version="20.3.0.59" />
	  <PackageReference Include="Syncfusion.Blazor.PdfViewerServer.Windows" Version="20.3.0.59" />
	  <PackageReference Include="Syncfusion.Blazor.Calendars" Version="20.3.0.59" />
	  <PackageReference Include="Syncfusion.Blazor.InPlaceEditor" Version="20.3.0.59" />
	  <PackageReference Include="Syncfusion.Blazor.DropDowns" Version="20.3.0.59" />
	  <PackageReference Include="Syncfusion.Blazor.Lists" Version="20.3.0.59" />
	  <PackageReference Include="Syncfusion.Blazor.Layouts" Version="20.3.0.59" />
	  <PackageReference Include="Syncfusion.Blazor.Cards" Version="20.3.0.59" />
	  <PackageReference Include="Syncfusion.Blazor.Inputs" Version="20.3.0.59" />
	  <PackageReference Include="Syncfusion.Blazor.Buttons" Version="20.3.0.59" />
	  <PackageReference Include="Syncfusion.Blazor.Navigations" Version="20.3.0.59" />
	  <PackageReference Include="Syncfusion.Blazor.Spinner" Version="20.3.0.59" />
	  <PackageReference Include="Syncfusion.XlsIO.Net.Core" Version="20.3.0.59" />
	  <PackageReference Include="Syncfusion.Pdf.Net.Core" Version="20.3.0.59" />
	  <PackageReference Include="Syncfusion.DocIO.Net.Core" Version="20.3.0.59" />
